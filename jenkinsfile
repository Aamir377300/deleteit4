pipeline{
    agent any{   // ❌ Incorrect syntax. 'agent any' should not have '{}'
        tools{   // ❌ Wrong placement. 'tools' should be inside 'pipeline', not inside 'agent'
            maven 'maven'
        }

        stages{  // ❌ 'stages' should be at the same level as 'agent', not inside it
            stage('Build'){
                steps{
                    sh 'mvn clean package'
                }
            }
            stage('Test'){
                steps{
                    sh 'mvn test'
                }
            }
        }
    }  // ❌ Extra closing bracket. 'agent any' should not contain nested elements
}